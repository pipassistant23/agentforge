{
  "name": "agentforge",
  "version": "1.0.0",
  "description": "Multi-agent orchestration platform. Telegram integration, baremetal execution, Agent Swarms.",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && cd agent-runner-src && npm run build && cd ..",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "release:check": "npm run typecheck && npm run format:check && npm test",
    "version": "node -e \"const fs=require('fs');const p=JSON.parse(fs.readFileSync('package.json','utf8'));console.log(p.version);\""
  },
  "dependencies": {
    "better-sqlite3": "^11.8.1",
    "cron-parser": "^5.5.0",
    "grammy": "^1.40.0",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.12",
    "@types/node": "^25.2.3",
    "@vitest/coverage-v8": "^4.0.18",
    "prettier": "^3.8.1",
    "tsx": "^4.19.0",
    "typescript": "^5.7.0",
    "vitest": "^4.0.18"
  },
  "engines": {
    "node": ">=20"
  }
}
